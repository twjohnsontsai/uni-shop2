@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}
.sugg-list {
  padding: 0 5px;
}
.sugg-list .sugg-item {
  font-size: 13px;
  padding: 13px 0;
  border-bottom: 1px solid #AFAFAF;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.sugg-list .sugg-item .sugg-name {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-right: 3px;
}
.histroy-box {
  padding: 0 5px;
}
.histroy-box .histroy-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 40px;
  font-size: 13px;
  border-bottom: 1px solid #efefef;
}
.histroy-box .histroy-list {
  display: flex;
  flex-wrap: wrap;
}
.histroy-box .histroy-list .uni-tag {
  margin-top: 5px;
  margin-left: 5px;
}

